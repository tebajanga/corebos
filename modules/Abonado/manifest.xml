<tables>
<table>
<name>vtiger_abonado</name>
<sql><![CDATA[CREATE TABLE `vtiger_abonado` (
  `abonadoid` int(11) NOT NULL,
  `active` varchar(25) default NULL,
  `numabonado` varchar(150) default NULL,
  `accid` int(19) default NULL,
  `fechaalta` date default NULL,
  `abgrado` varchar(200) default NULL,
  `fechabaja` date default NULL,
  `pclave` varchar(250) default NULL,
  `pcoaccion` varchar(250) default NULL,
  `pbatic` varchar(250) default NULL,
  `abtipolocal` varchar(200) default NULL,
  `horaapprox` datetime default NULL,
  `operativa` varchar(200) default NULL,
  `datoslistado` varchar(200) default NULL,
  `accesoweb` varchar(200) default NULL,
  `obsdir` varchar(200) default NULL,
  `marca` int(19) NOT NULL,
  `tipopanel` int(19) NOT NULL,
  `snpanel` int(19) NOT NULL,
  `transgprs` varchar(25) default NULL,
  `simgprs` varchar(200) default NULL,
  `opgprs` varchar(200) default NULL,
  `transiprtc` varchar(25) default NULL,
  `domiprtc` varchar(200) default NULL,
  `cctv` varchar(25) default NULL,
  `camip` int(19) NOT NULL,
  `marcacctv` int(19) NOT NULL,
  `calarma` varchar(200) default NULL,
  `cbzone_id` int(11) NULL,
  `cto_id` int(11) NULL,
  `abonado_no` varchar(250) default NULL,
  `optel` varchar(50) default NULL,
  PRIMARY KEY (`abonadoid`),
  CONSTRAINT `fk_1_vtiger_abonabo` FOREIGN KEY (`abonadoid`) REFERENCES `vtiger_crmentity` (`crmid`) ON DELETE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
<table>
<name>vtiger_abonadocf</name>
